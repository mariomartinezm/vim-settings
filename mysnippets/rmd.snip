snippet     cell
options     head
    \`\`\`{r}
    ${1}
    \`\`\`

snippet     cellfig
options     head
    \`\`\`{r fig.align=${1:"center"}, fig.width=${2:7}, fig.height=${3:7}}
    ${4}
    \`\`\`

snippet     header
options     head, indent
	---
	title: ${1}
	autnor: ${2}
    output:
		html_document:
			theme: ${3:united}
			highlight: ${4:zenburn}
			df_print: ${5:paged}
	---
    ${5}
